{
  "name": "@empjs/valtio",
  "version": "1.0.0",
  "description": "valtio store",
  "license": "MIT",
  "type": "module",
  "files": ["dist"],
  "maintainers": ["ckken"],
  "publishConfig": {
    "access": "public"
  },
  "keywords": ["valtio", "store"],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Ken",
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "build:tsup": "tsup --config tsup.config.ts",
    "dev:tsup": "tsup --watch --config tsup.config.ts",
    "test": "bun test",
    "tc": "bun test --coverage"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^14.0.0",
    "@testing-library/react": "^14.2.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "derive-valtio": "^0.2.0",
    "valtio": "^2.3.0",
    "valtio-history": "^1.0.1"
  }
}
